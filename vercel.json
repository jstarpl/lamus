{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/404.ts"
    },
    {
      "source": "/text",
      "destination": "/"
    },
    {
      "source": "/files",
      "destination": "/"
    },
    {
      "source": "/code",
      "destination": "/"
    },
    {
      "source": "/code/io/:path*",
      "destination": "/"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "./api/*.json"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/up_/src/serviceWorker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "buildCommand": "yarn build",
  "installCommand": "yarn",
  "devCommand": "yarn start",
  "outputDirectory": "./lamus-mvp/build"
}