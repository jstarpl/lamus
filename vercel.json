{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/404.ts"
    },
    {
      "source": "/text",
      "destination": "/"
    },
    {
      "source": "/files",
      "destination": "/"
    },
    {
      "source": "/code",
      "destination": "/"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "./api/*.json"
    }
  },
  "buildCommand": "yarn build",
  "installCommand": "yarn",
  "devCommand": "yarn start",
  "outputDirectory": "./lamus-mvp/build"
}