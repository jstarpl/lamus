{
  "eslint.workingDirectories": [
    "api",
    "codemirror-lang-qbasic-vm",
    "lamus-mvp",
    "qbasic-vm",
    "setup",
    "docs"
  ],
  "editor.codeActionsOnSave": [
    "source.fixAll.eslint",
    "source.organizeImports"
  ],
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": false,
  "liveServer.settings.root": "/lamus-mvp/build",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/yarn.lock": true,
    ".yarn/**/*": true,
  }
}
