{
  "name": "@lamus/lamus-mvp",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/commands": "^0.20.0",
    "@codemirror/lang-javascript": "^0.20.1",
    "@codemirror/state": "^0.20.1",
    "@codemirror/view": "^0.20.7",
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.7.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.24.3",
    "@editorjs/header": "^2.6.2",
    "@editorjs/list": "^1.7.0",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.9.0",
    "@editorjs/quote": "^2.4.0",
    "@emoji-mart/data": "^1.0.1",
    "@lamus/codemirror-lang-qbasic-vm": "*",
    "@lamus/qbasic-vm": "*",
    "@meshtastic/meshtasticjs": "^0.9.8",
    "@sofie-automation/sorensen": "^1.4.2",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "dropbox": "^10.29.0",
    "emoji-mart": "^5.0.0",
    "framer-motion": "^6.3.10",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.0.1",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.4.0",
    "node-poweredup": "^8.0.5",
    "picomatch": "^2.3.1",
    "pretty-bytes": "^6.0.0",
    "prompts": "^2.4.2",
    "qrcode.react": "^3.0.2",
    "react": "^18.1.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.1.0",
    "react-editor-js": "^2.0.6",
    "react-icons": "^4.6.0",
    "react-markdown": "^8.0.3",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.3.0",
    "react-spinners": "^0.13.4",
    "remark": "^14.0.2",
    "semver": "^7.3.5",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "webdav": "^4.10.0"
  },
  "scripts": {
    "start": "parcel ./public/index.html --dist-dir ./build",
    "build:libs": "cd ../qbasic-vm && npm install && yarn bundle && cd ../../codemirror-lang-qbasic-vm && yarn && yarn prepare",
    "build:app": "parcel build ./public/index.html --dist-dir ./build",
    "build": "yarn build:libs && yarn build:app",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "linebreak-style": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lezer/generator": "^1.0.0",
    "@parcel/config-default": "^2.8.3",
    "@parcel/transformer-raw": "^2.8.3",
    "@types/emoji-mart": "^3.0.9",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^3.0.0",
    "@types/picomatch": "^2.3.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "parcel": "^2.8.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
